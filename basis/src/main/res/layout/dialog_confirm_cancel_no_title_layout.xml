<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/dp_40"
    android:layout_marginEnd="@dimen/dp_40"
    android:background="@drawable/r7_white_shape"
    android:paddingBottom="@dimen/dp_30">

    <TextView
        android:id="@+id/dialogNoTitleContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_35"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="@dimen/dp_35"
        android:text="确定要清楚缓存吗？"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dialogNoTitleCancel"
        android:layout_width="105dp"
        android:layout_height="@dimen/dp_33"
        android:layout_marginStart="@dimen/dp_27"
        android:layout_marginTop="@dimen/dp_35"
        android:background="@drawable/r40_x707070_shape"
        android:gravity="center"
        android:text="取消"
        android:textColor="@color/color_707070"
        android:textSize="@dimen/sp_15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogNoTitleContent" />

    <TextView
        android:id="@+id/dialogNoTitleOk"
        android:layout_width="105dp"
        android:layout_height="@dimen/dp_33"
        android:layout_marginTop="@dimen/dp_35"
        android:layout_marginEnd="@dimen/dp_27"
        android:background="@drawable/r40_c3292ff_shape"
        android:gravity="center"
        android:text="确定"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogNoTitleContent" />

</androidx.constraintlayout.widget.ConstraintLayout>