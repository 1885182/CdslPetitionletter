<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View android:id="@+id/itemEventDetailedCheckLine"
        android:layout_width="@dimen/line_height"
        android:layout_height="0dp"
        android:background="@color/color_dc3f3f"
        app:layout_constraintBottom_toTopOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintEnd_toEndOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintStart_toStartOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintTop_toTopOf="parent" />

    <View android:id="@+id/itemEventDetailedCheckLine2"
        android:layout_width="@dimen/line_height"
        android:layout_height="0dp"
        android:background="@color/color_dc3f3f"
        app:layout_constraintBottom_toBottomOf="@+id/itemEventDetailedGridNameLL"
        app:layout_constraintEnd_toEndOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintStart_toStartOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintTop_toBottomOf="@+id/itemEventDetailedCheckIv" />

    <ImageView
        android:id="@+id/itemEventDetailedCheckIv"
        android:layout_width="@dimen/dp_12"
        android:layout_height="@dimen/dp_12"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_18"
        android:src="@mipmap/red_select"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/itemEventDetailedGridName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_4"
        android:text="曲周县"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintBottom_toBottomOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintStart_toEndOf="@+id/itemEventDetailedCheckIv"
        app:layout_constraintTop_toTopOf="@+id/itemEventDetailedCheckIv" />

    <LinearLayout
        android:id="@+id/itemEventDetailedGridNameLL"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_15"
        android:background="@drawable/r4_cf2f2f2_shape"
        android:orientation="vertical"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="15dp"
        android:paddingBottom="@dimen/dp_16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/itemEventDetailedGridName"
        app:layout_constraintTop_toBottomOf="@+id/itemEventDetailedGridName">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/itemEventDetailedReportName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_8"
                android:layout_weight="3.3"
                android:textColor="@color/color_030303"
                android:textSize="@dimen/sp_14"
                tools:text="202105280003" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/itemEventDetailedReportTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_8"
                android:layout_weight="3.3"
                android:textColor="@color/color_030303"
                android:textSize="@dimen/sp_14"
                tools:text="202105280003" />


        </LinearLayout>

            <TextView
                android:id="@+id/itemEventDetailedReportGo"
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_8"
                android:layout_weight="3.3"
                android:textColor="@color/color_030303"
                android:textSize="@dimen/sp_14"
                tools:text="202105280003" />



            <TextView
                android:id="@+id/itemEventDetailedFour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginStart="@dimen/dp_8"
                android:layout_weight="3.3"
                android:textColor="@color/color_030303"
                android:textSize="@dimen/sp_14"
                tools:text="202105280003" />



        <LinearLayout
            android:id="@+id/itemEventDetailedSatisfactionLL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="满意度:"
                android:textColor="@color/color_ababab"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:id="@+id/itemEventDetailedSatisfaction"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_18"
                android:layout_weight="3.3"
                android:textColor="@color/color_030303"
                android:textSize="@dimen/sp_14"
                tools:text="202105280003" />


        </LinearLayout>



    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>